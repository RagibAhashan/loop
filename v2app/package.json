{
    "name": "app",
    "version": "0.1.0",
    "author": "Dynasty AIO",
    "private": true,
    "description": "A bot",
    "homepage": "./",
    "main": "./src/main.prod.js",
    "dependencies": {
        "@craco/craco": "^6.1.1",
