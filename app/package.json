{
    "name": "app",
    "version": "0.1.0",
    "author": "Dynasty AIO",
    "private": true,
    "description": "A bot",
    "homepage": "./",
    "main": "./src/main.prod.js",
    "dependencies": {
        "@material-ui/core": "^4.11.2",
        "@testing-library/jest-dom": "^5.11.4",
        "@testing-library/react": "^11.1.0",
        "@testing-library/user-event": "^12.1.10",
        "@types/jest": "^26.0.15",
        "@types/node": "^12.0.0",
        "@types/react": "^16.9.53",
        "@types/react-dom": "^16.9.8",
        "antd": "^4.10.3",
        "react": "^17.0.1",
        "react-dom": "^17.0.1",
        "react-scripts": "4.0.1",
        "typescript": "^4.0.3"
    },
    "scripts": {
        "start:react": "cross-env BROWSER=none react-scripts start",
        "build": "react-scripts build",
        "test": "react-scripts test",
        "eject": "react-scripts eject",
        "start:electron": "webpack --config webpack.config.dev.js && cross-env NODE_ENV=prod electron .",
        "package": "webpack --config webpack.config.prod.js && electron-builder build"
    },
    "eslintConfig": {
        "extends": [
            "react-app",
            "react-app/jest"
        ]
    },
    "browserslist": {
        "production": [
            ">0.2%",
            "not dead",
            "not op_mini all"
        ],
        "development": [
            "last 1 chrome version",
            "last 1 firefox version",
            "last 1 safari version"
        ]
    },
    "devDependencies": {
        "concurrently": "^5.3.0",
        "cross-env": "^7.0.3",
        "electron": "^11.2.0",
        "electron-builder": "^22.9.1",
        "webpack-cli": "^4.4.0"
    },
    "build": {
        "extends": null,
        "files": [
            "./build/**/*",
            "./build/index.html",
            "src/main.prod.js"
        ],
        "directories": {
            "output": "release",
            "buildResources": "./src/assets"
        },
        "productName": "Dynasty AIO",
        "appId": "com.dynasty",
        "copyright": "Copyright Â© 2021 ${author}"
    }
}
